[alias]
  co = checkout
  cm = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[color]
  diff = auto
  status = auto
  branch = auto
[core]
	excludesfile = ~/.gitignore_global
